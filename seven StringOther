package seven;

public class StringOther {
    public static void main(String[] arg){
        float f = 56.1234f;
        String str = String.valueOf(f);
        int idx = str.indexOf('.');
        String sf =str.substring(0,idx+3);
        System.out.println("f=" +sf);
    }
}
